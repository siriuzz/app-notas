{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "overrides": [
    {
      "files": ["*-test.js","*.spec.js","*.js"],
      "rules": {
        "no-unused-expressions": "off",
        "quotes":"off",
        "max-len":"off",
      }
    }
